<%- include('partials/header') %>

<div class="container-fluid bg-dark">
  <div class="row">
    <div class="col-sm-2">
      <%- include('partials/same',{page_name:'contact'}) %>
    </div>
    <div class="col-sm-10">
      <div class="container-fluid border-1 border-bottom">
        <h2 class="services mt-4">Contact Us</h2>
      </div>

      <p class="text-white fs-5 mt-4">
        do you have any questions? please do not hesitate to contact.i will come
        back to you within a matter of hours to help you
      </p>

      <div class="row mt-5">
        <div class="col-sm-10">
          <div class="row">
            <div class="col-sm-6 mb-3 mt-2">
              <div class="mb-3">
                <label
                  for="exampleFormControlInput1"
                  class="form-label text-white"
                  >Enter Name</label
                >
                <input
                  type="text"
                  class="form-control"
                  id="exampleFormControlInput1"
                  placeholder="Enter Your Name"
                />
              </div>
            </div>
            <div class="col-sm-6 mb-3 mt-2">
              <div class="mb-3">
                <label
                  for="exampleFormControlInput1"
                  class="form-label text-white"
                  >Email address</label
                >
                <input
                  type="email"
                  class="form-control"
                  id="exampleFormControlInput1"
                  placeholder="name@example.com"
                />
              </div>
            </div>

            <!-- 2nd -->
            <div class="col-sm-12 mb-3 mt-2">
              <div class="mb-3">
                <label
                  for="exampleFormControlInput1"
                  class="form-label text-white"
                  >Your Subject</label
                >
                <input
                  type="text"
                  class="form-control"
                  id="exampleFormControlInput1"
                  placeholder="Your Subject "
                />
              </div>
            </div>

            <!-- 3rd -->
            <div class="col-sm-12 mb-3 mt-2">
              <div class="mb-3">
                <label
                  for="exampleFormControlTextarea1"
                  class="form-label text-white"
                  >Your Message</label
                >
                <textarea
                  class="form-control"
                  id="exampleFormControlTextarea1"
                  rows="3"
                ></textarea>
              </div>
            </div>
          </div>
          <button class="bg-success">Send</button>
        </div>

        <div class="col-sm-2 text-center">
          <div class="row text-white">
            <div class="col-sm-12 mt-3">
              <div class="iconcolor"><i class="fas fa-location-arrow"></i></div>
              <div>Razabad Chowk Jinnah Town</div>
            </div>

            <div class="col-sm-12 mt-5">
              <div class="iconcolor"><i class="fas fa-phone-square"></i></div>
              <div>301-6957648</div>
            </div>

            <div class="col-sm-12 mt-5">
              <div class="iconcolor"><i class="fas fa-address-card"></i></div>
              <div>waqasishaq800@gmail.com</div>
            </div>
          </div>
        </div>

        <div class="footer container-fluid">
          <div class="flex23 text-center text-white">
            <div><i class="fab fa-github"></i></div>
            <div><i class="fab fa-linkedin-in"></i></div>
            <div><i class="fab fa-medium"></i></div>
            <div><i class="fab fa-facebook"></i></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<%- include('partials/footer') %>
